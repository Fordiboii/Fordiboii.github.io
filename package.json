{
  "name": "magno-motion-test",
  "version": "0.0.1",
  "description": "Motion detection test used for dyslexia research.",
  "scripts": {
    "test": "npx jest",
    "code-coverage": "jest --coverage",
    "dev": "parcel ./src/index.html --port=8080 --open ",
    "prebuild": "npx del-cli ./dist",
    "build": "cross-env NODE_ENV=production parcel build ./src/index.html --public-url .",
    "deploy": "gh-pages -d dist"
  },
  "author": "Fordiboii",
  "license": "MIT",
  "dependencies": {
    "cross-env": "^7.0.3",
    "gh-pages": "^3.1.0",
    "mainloop.js": "^1.0.4",
    "pixi-filters": "^4.0.0",
    "pixi.js": "^5.3.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "@types/jsdom": "^16.2.3",
    "@types/mainloop.js": "^1.0.5",
    "del-cli": "^3.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.5.4",
    "jest-canvas-mock": "^2.2.0",
    "jsdom": "^16.2.2",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-static-files-copy": "^2.4.3",
    "ts-jest": "^26.1.1",
    "typescript": "^3.9.5"
  },
  "staticFiles": {
    "staticPath": [
      {
        "staticPath": "./src/assets",
        "staticOutDir": "./assets"
      }
    ],
    "watcherGlob": "**"
  },
  "browserslist": [
    "since 2017-06"
  ]
}
